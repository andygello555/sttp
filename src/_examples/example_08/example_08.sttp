fun fib(n)
    is n < 2?
        return n;
    end
    $print(n - 1, n - 2);
    return $fib(n - 1) + $fib(n - 2);
end

a = {"n": 7};

fun a.fib()
    is self.n < 2?
        return self.n;
    end

    b.n = self.n - 1;
    b.fib = self.fib;
    fib1 = $b.fib();

    c.n = self.n - 2;
    c.fib = self.fib;
    fib2 = $c.fib();
    $print(b.n, c.n);

    return fib1 + fib2;
end

fib7 = $fib(7);
$print("fib7 =", fib7);

$print();

a.fib7 = $a.fib();
$print("a.fib7 =", a.fib7);
