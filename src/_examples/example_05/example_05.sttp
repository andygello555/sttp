out = "";

// Traditional for loops
for i = 0; i < 100; i = i + 1 do
    if i % 3 == 0 && i % 5 == 0 then
        out = out + "FizzBuzz";
    elif i % 3 == 0 then
        out = out + "Fizz";
    elif i % 5 == 0 then
        out = out + "Buzz";
    else
        out = out + i;
    end;
end;

$print(out);

for i = 0; i < 100; i = i + 1 do
    if i == 50 then
        $print("Found 50, now we take a break...");
        break;
    end;
    $print("On %%..." % i);
end;

