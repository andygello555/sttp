out = "";

// Traditional for loops
for i = 0; i < 100; i = i + 1 do
    is i % 3 == 0 && i % 5 == 0?
        out = out + "FizzBuzz";
    elis i % 3 == 0?
        out = out + "Fizz";
    elis i % 5 == 0?
        out = out + "Buzz";
    else
        out = out + i;
    end
end

$print(out);

for i = 0; i < 100; i = i + 1 do
    is i == 50?
        $print("Found 50, now we take a break...");
        break;
    end
    $print("On %%..." % i);
end
